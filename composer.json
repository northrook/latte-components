  "type": "library",
  "description": "Manage document meta, enqueue header assets, handle session notifications, custom properties and more.",
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "Northrook\\Latte\\Component\\": "components",
      "Northrook\\Latte\\": ""
    }
  },
  "authors": [
    {
      "name": "Martin",
      "email": "mn@northrook.com"
    }
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": ">=8.2",
    "northrook/core": "dev-main",
    "northrook/html": "dev-main",
    "northrook/latte-bundle": "dev-main",
    "symfony/http-foundation": "@stable",
    "symfony/security-core": "@stable",
    "northrook/icon-manager": "dev-main"
  },
  "require-dev": {
    "phpstan/phpstan": " @stable",
    "northrook/dev-env": "dev-main"
  }
}
